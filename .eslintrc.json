{
    "extends": "airbnb",
    "env": {
      "es6": true,
      "browser": true
    },
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "15.0"
      }
    },
    "rules": {
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "id-length": 0,
      "react/jsx-indent": [
        2,
        2
      ],
      "react/jsx-first-prop-new-line": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/jsx-indent-props": [
        2,
        2
      ],
      "max-len": [
        "error",
        120
      ],
      "consistent-return": 0,
      "no-param-reassign": 0,
      "import/no-unresolved": 0,
      "import/extensions": ["off", "never"]
    }
  }